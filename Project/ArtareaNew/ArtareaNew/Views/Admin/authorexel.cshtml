
@using (Html.BeginForm("authorexel", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <table class="form-control">
        <tr class="form-group">
            
            <td>File:</td>
            <td >
                <input type="file" name="UploadedFile"/>
            </td>
        </tr>
        <tr class="form-group">
            <td colspan="2">
                <input type="submit" name="Submit" value="Submit"  class="btn btn-success" style="margin-left:30px;"/>
            </td>
        </tr>
    </table>
}

@ViewBag.result
@ViewBag.count
