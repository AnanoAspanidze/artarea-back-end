@model IEnumerable<ArtareaNew.Models.CategoryForAdmin>

@{
    ViewBag.Title = "categoryexel";
}




@{
    ViewBag.Title = "addCategory";
}

<h2>addcategory</h2>


@using (Html.BeginForm("categoryexel", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
{

    <div class="form-horizontal" style="margin-left:60px; margin-top:20px;">
        <h4>შეიტანეთ ინფორმაცია ექსელის ფაილიდან</h4>

        <div class="form-group">
            <input type="file" name="UploadedFile" class="" />
        </div>
        <div class="form-group">
            <div>
                <input type="submit" name="Submit" value="Submit" class="btn btn-success " />
            </div>
        </div>
        @ViewBag.result
        @ViewBag.result1
    </div>
}



@using (Html.BeginForm("addCategory", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
{

    <div class="form-horizontal" style="margin-left:50px; margin-top:100px;">
        <h4>შეიტანეთ ინფორმაცია ხელით</h4>

        <div class="form-group" style="">
            <div class="col-md-10">
                <input type="text" name="Name" class="form-control" placeholder="Name" />
            </div>
        </div>

        <div class="form-group">
            <div class="col-lg-pull-9">
                <input type="submit" name="Submit" value="Submit" class="btn btn-success" style="margin-left:30px;" />
            </div>
        </div>

    </div>

}


<table class="table">
        <tr>
            <th>
                name
            </th>
            <th>
                leCode
            </th>
            <th></th>
        </tr>
        @if (Model is null) { }
        else
        {
            foreach (var item in Model)
            {
                <tr>
                    <td>
                        @item.Name
                    </td>
                    <td>
                        @item.leCode
                    </td>

                </tr>
            }
        }

    </table>
